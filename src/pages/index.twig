{% extends '../blocks/layout/layout.twig' %}{% set newsList = [{href: '#', txt: 'Aleshka'}, {href: '#', txt: 'Aleshka'}, {href: '#', txt: 'Aleshka'}]  %}{% set featuresList = [{icon: 'Arrow', label: '1 + 1', para: 'Вышел заяц'}, {icon: 'Arrow', label: '1 + 1', para: 'Вышел заяц'}] %}{% block content %}    {% include '../blocks/banner/banner.twig' %}    <main class="main">        <div class="main__i">            <section class="last-pubs">                {% include '../blocks/title/title.twig' with {                    h2: true,                    titleTxt: 'Последние публикации'                }%}                <div class="last-pub__list">                    {% embed '../blocks/news-list/news-list.twig' %}                    {% block content %}                        {% for item in newsList %}                    <li class="news-list__item">                        <time class="news-list__date">12.01.2017</time>                        <a href="{{ item.href }}" class="news-list__link">{{ item.txt }}</a>                        <a href="#" class="news-list__more">more</a>                    </li>                    {% endfor %}                    {% endblock %}                    {% endembed %}                </div>                <div class="last-pubs__subscribe">                    <section class="subscribe">                        <div class="subscribe__title">                            {% include '../blocks/title/title.twig' with {                            h3: true,                            titleTxt: 'Подпишитесь и получайте свежие статьи на почту'                            } %}                        </div>                        <div class="subscribe__i">                            <div class="subscribe__input">                                {% include '../blocks/input/input.twig' with {                                    inputValue: 'aleshka'                                } %}                            </div>                            <div class="subscribe__input">                                {% include '../blocks/input/input.twig' with {                                    inputValue: 'dimon'                                } %}                            </div>                            <div class="subscribe__btn">                                {% include '../blocks/button/button.twig' with {                                    btnTxt: 'Подписаться',                                    btnClass: 'btn--green'                                }%}                            </div>                        </div>                    </section>                </div>            </section>            <section class="features">                <div class="features__title">                    {% include '../blocks/title/title.twig' with {                    h2: true,                    titleTxt: 'Отличия и пеимущество'                    }%}                  </div>                <div class="features__list">                    {% include '../blocks/features-list/features-list.twig' %}                </div>                <footer class="features__footer">                    <span class="features__descr">Кошель на любую платформу</span>                    <div class="features__btn">                        {% include '../blocks/button/button.twig' with {                            btnTxt: 'Скачать кошелек'                        } %}                    </div>                </footer>            </section>            <section class="goto">                <div class="goto__title">                    {% include '../blocks/title/title.twig' with {                        h2: true,                        titleTxt: 'Перейти в раздел'                    }%}                </div>                <ul class="goto__list">                    <li class="goto__item">                        <a href="" class="goto__link">Для новичков</a>                    </li>                    <li class="goto__item">                        <a href="" class="goto__link">Для экспертов</a>                    </li>                </ul>                <div class="goto__btn">                    {% include '../blocks/button/button.twig' with {                        btnTxt: 'Смотреть котировки'                    } %}                </div>            </section>        </div>    </main>{% endblock content %}